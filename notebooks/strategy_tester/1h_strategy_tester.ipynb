   "metadata": {},
   "outputs": [],
   "source": [
    "STRATEGY_NAME = 'ema112'\n",
    "strategy_func = load_strategy(STRATEGY_NAME)\n",
    "\n",
    "props = StrategyProperties(\n",
    "    symbol='BTCUSDT',\n",
    "    timeframe='1H',\n",
    "    initial_capital=10_000.0,\n",
    "    trading_cost_bps=5.0,\n",
    "    stop_loss_pct=None,\n",
    ")\n",
    "\n",
    "positions = strategy_func(df)\n",
    "backtest = run_backtest(df, positions, **props.to_kwargs())\n",
    "backtest.head()\n"
   ]
